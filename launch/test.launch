<launch>
    <arg name="map_file" default="$(find ros_algorithms)/map/house_test.yaml"/>
    <arg name="print_time" default="true"/>
    <arg name="global_planner_algorithm" default="astar"/>

    <param name="print_time" value="$(arg print_time)"/>
    <param name="global_planner_algorithm" value="$(arg global_planner_algorithm)"/>

    <node pkg="GlobalPlanner" type="global_planner_node" name="global_planner_node" output="screen"/>
    <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)" respawn="true"/>
    <node pkg="rviz" name="rviz" type="rviz" args="-d $(find ros_algorithms)/rviz/path_test.rviz"/>
</launch>